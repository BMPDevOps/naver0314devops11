package java0315;

import java.util.Scanner;

public class Ex12_Munje {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		/*
		 * 점수가 90이상이면 "참 잘했어요"
		 * 		  80이상이면 "수고했어"
		 * 		  70이상이면 "좀 더 노력하세요"
		 * 		  나머지는 "재시험입니다."
		 */
		
		int score;
		System.out.println("점수 입력");
		score = sc.nextInt();
		

	}

}
